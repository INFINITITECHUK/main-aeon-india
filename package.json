{
  "name": "customer",
  "version": "0.2.0",
  "description": "",
  "author": "Mohan Bhandari",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:uat": "NODE_ENV=uat nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=prod node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint-fix": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/typeorm.config.ts",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "typeorm:create": "npm run typeorm migration:create -- -n",
    "typeorm:run": "npm run typeorm migration:run"
  },
  "dependencies": {
    "@nestjs/bull": "^0.3.1",
    "@nestjs/cli": "^7.5.4",
    "@nestjs/common": "^7.6.3",
    "@nestjs/core": "^7.6.3",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/microservices": "^7.6.3",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.3",
    "@nestjs/schedule": "^0.4.1",
    "@nestjs/swagger": "^4.7.8",
    "@nestjs/typeorm": "^7.1.0",
    "argon2": "^0.27.1",
    "axios": "^0.21.0",
    "bull": "^3.20.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "compression": "^1.7.4",
    "cron": "^1.8.2",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "helmet": "^4.2.0",
    "moment": "^2.29.1",
    "nest-winston": "^1.4.0",
    "nestjs-i18n": "^8.0.11",
    "nestjs-minio": "^2.0.10",
    "node-oracledb": "^1.0.2",
    "oracledb": "^5.1.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "swagger-ui-express": "^4.1.5",
    "typeorm": "0.2.25",
    "typeorm-encrypted": "^0.5.4",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "xlsx": "^0.16.9",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@nestjs/schematics": "^7.2.5",
    "@nestjs/testing": "^7.6.3",
    "@types/bull": "^3.14.4",
    "@types/compression": "^1.7.0",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.9",
    "@types/helmet": "0.0.48",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "4.10.0",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "2.22.1",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "typescript": "4.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coverageReporters": [
      "text",
      "html"
    ]
  },
  "snyk": true
}
